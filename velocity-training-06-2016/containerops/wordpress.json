{
	"id": "wordpress",
	"container": {
		"type": "DOCKER",
		"docker": {
			"image": "tutum/wordpress:latest",
			"network": "BRIDGE",
			"portMappings": [
				{
					"containerPort": 80,
					"hostPort": 0,
					"servicePort": 10000
				}
			]
		}
	},
	"labels": {
		"HAPROXY_GROUP": "external",
		"HAPROXY_0_VHOST": "http://ec2-52-26-193-12.us-west-2.compute.amazonaws.com"
	},
	"cpus": 1,
	"mem": 256.0,
	"instances": 1,
	"healthChecks": [
		{
			"path": "/",
			"portIndex": 0,
			"protocol": "HTTP",
			"gracePeriodSeconds": 120,
			"intervalSeconds": 10,
			"timeoutSeconds": 40,
			"maxConsecutiveFailures": 5
		}
	]
}